{
  "name": "spotishush",
  "version": "2022.5.9",
  "license": "MIT",
  "author": "guihkx",
  "devDependencies": {
    "standard": "16.0.4",
    "web-ext": "6.8.0"
  },
  "scripts": {
    "build": "web-ext build --config ./web-ext-config.js",
    "standard:check": "standard --verbose ./src/**/*.js",
    "standard:fix": "standard --fix --verbose ./src/**/*.js",
    "start:chromium": "web-ext run --target chromium --config ./web-ext-config.js --no-config-discovery",
    "start:firefox": "web-ext run --target firefox-desktop --config ./web-ext-config.js --no-config-discovery"
  },
  "standard": {
    "envs": [
      "browser",
      "webextensions"
    ]
  }
}
